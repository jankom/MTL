start
  = line+

line
  = opener linecont+ "\n"*

linecont
  = word / urgent / space / special 

special
  = "[" (progress / attime) "]"

progress
  = [+-]+

attime
  = "at " [0-9]+ (":" [0-9]+ / "h")

word
  = [a-zA-Z]+

space
  = " "+

opener                                                                                                                      
  = "#"+/"+"+/"-"+/"~"+/

urgent
  = "!"+
